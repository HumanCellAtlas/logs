{
   "Code": {
      "S3Bucket": "hca-code-deployment",
      "S3Key": "GcpToCloudWatchLogs_App.zip"
   },
   "Description": "Exports GCP Stackdriver Logs to CloudWatch Logs",
   "Environment": {
      "Variables": {
        "GCLOUD_CREDENTIALS": "${GCLOUD_CREDENTIALS}",
        "GCLOUD_PROJECT": "${GCLOUD_PROJECT}",
        "GOOGLE_APPLICATION_CREDENTIALS": "${GOOGLE_APPLICATION_CREDENTIALS}",
        "LOG_TOPIC_SUBSCRIPTION_NAME": "${LOG_TOPIC_SUBSCRIPTION_NAME}",
        "REGION": "${REGION}"
      }
   },
   "FunctionName": "GcpToCloudWatchLogs",
   "Handler": "index.handler",
   "MemorySize": 128,
   "Publish": true,
   "Role": "${GCP_EXPORT_ROLE_ARN}",
   "Runtime": "nodejs6.10",
   "Timeout": 60
}
